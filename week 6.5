def find_mapping(A, B):
    index_map = {}
    
    for i, num in enumerate(B):
        index_map[num] = i
    mapping = []
    for num in A:
        mapping.append(index_map[num])
    
    return mapping
n = int(input())
A = list(map(int, input().split()))
B = list(map(int, input().split()))
print(*find_mapping(A, B))
